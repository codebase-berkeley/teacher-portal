{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "jsx-a11y/label-has-for": [
      2,
      {
        "required": {
          "every": ["id"]
        }
      }
    ],
    "prettier/prettier": ["error"],
    "react/prefer-stateless-function": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}